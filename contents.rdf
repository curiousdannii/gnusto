  </RDF:Seq>

  <!-- Mozilla -->
  <RDF:Seq about="chrome://communicator/content/tasksOverlay.xul">
    <RDF:li>chrome://gnusto/content/gnustooverlay_moz.xul</RDF:li>
  </RDF:Seq>

</RDF:RDF>