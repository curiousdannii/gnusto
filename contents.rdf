    <RDF:li resource="urn:mozilla:package:gnusto"/>
  </RDF:Seq>

  <!-- package information -->
  <RDF:Description about="urn:mozilla:package:gnusto"
        chrome:displayName="gnusto"
        chrome:author="marnanel@marnanel.org"
